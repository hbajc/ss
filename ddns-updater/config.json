{
  "name": "DDNS Updater",
  "version": "2.9",
  "slug": "ddns_updater",
  "description": "Automatically update DNS records for dynamic IP addresses",
  "url": "https://github.com/qdm12/ddns-updater",
  "arch": ["amd64", "armhf", "armv7", "aarch64", "i386"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "8000/tcp": 8000
  },
  "ports_description": {
    "8000/tcp": "Web interface port"
  },
  "map": ["config:rw"],
  "options": {
    "settings": [
      {
        "provider": "str",
        "domain": "str",
        "host": "str?",
        "ip_version": "str?",
        "ipv6_suffix": "str?",
        "token": "str?",
        "username": "str?",
        "password": "str?",
        "key": "str?",
        "secret": "str?",
        "proxied": "bool?"
      }
    ]
  },
  "schema": {
    "settings": [
      {
        "provider": "str",
        "domain": "str",
        "host": "str?",
        "ip_version": "str?",
        "ipv6_suffix": "str?",
        "token": "str?",
        "username": "str?",
        "password": "str?",
        "key": "str?",
        "secret": "str?",
        "proxied": "bool?"
      }
    ]
  },
  "image": "ghcr.nju.edu.cn/qdm12/ddns-updater",
  "environment": {
    "DATADIR": "/data",
    "CONFIG_FILEPATH": "/data/config.json"
  }
}
