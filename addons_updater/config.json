{
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "boot": "manual",
  "codenotary": "alexandrep.github@gmail.com",
  "description": "自动通过与上游发布版本标签对齐来更新附加组件。",
  "environment": {
    "GIT_DISCOVERY_ACROSS_FILESYSTEM": "1"
  },
  "image": "ghcr.nju.edu.cn/alexbelgium/addons_updater-{arch}",
  "init": false,
  "map": [
    "addon_config:rw"
  ],
  "name": "Repository Updater",
  "options": {
    "gitapi": "gitapi",
    "gituser": "gituser",
    "repository": "alexbelgium/hassio-addons"
  },
  "schema": {
    "dry_run": "bool?",
    "gitapi": "str",
    "gitmail": "str?",
    "gituser": "str",
    "repository": "str",
    "verbose": "bool?"
  },
  "slug": "updater",
  "udev": true,
  "url": "https://ghfast.top/github.com/alexbelgium/hassio-addons/tree/master/addons_updater",
  "version": "3.19.10"
}
