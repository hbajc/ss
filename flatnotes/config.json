{
  "arch": [
    "amd64",
    "aarch64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "description": "一个自托管的、无数据库的笔记应用程序，使用扁平化的markdown文件存储。",
  "environment": {
    "FLATNOTES_PATH": "/config/addons_config/flatnotes",
    "PUID": "0",
    "PGID": "0"
  },
  "map": [
    "config:rw"
  ],
  "name": "flatnotes",
  "options": {
    "FLATNOTES_AUTH_TYPE": "none"
  },
  "schema": {
    "FLATNOTES_AUTH_TYPE": "list(none|read_only|password|totp)",
    "FLATNOTES_USERNAME": "str?",
    "FLATNOTES_PASSWORD": "str?",
    "FLATNOTES_SECRET_KEY": "str?",
    "FLATNOTES_SESSION_EXPIRY_DAYS": "int?",
    "FLATNOTES_TOTP_KEY": "str?"
  },
  "ports": {
    "8080/tcp": "8080"
  },
  "ports_description": {
    "8080/tcp": "WebUI"
  },
  "slug": "flatnotes",
  "startup": "application",
  "upstream": "1.0",
  "url": "https://gh-proxy.com/github.com/jdeath/homeassistant-addons/tree/main/flatnotes",
  "version": "v5.5.1",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8080]",
  "init": false,
  "ingress": false
}
